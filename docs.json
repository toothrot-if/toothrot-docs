{
    "name": "Toothrot",
    "output": "dist/docs/",
    "themeDir": "node_modules/toothrot-allthedocs/theme/",
    "ignore": [
        "dist/",
        "node_modules/",
        "resources/",
        "src/",
        "bin/",
        "build/",
        "\\.js$",
        "docs/theme/"
    ],
    "navigation": {
        "Home": "index.md",
        "Documentation": "docs/user/index.md",
        "Community": "docs/community.md",
        "GitHub": "https://github.com/toothrot-if/toothrot",
        "Reddit": "https://reddit.com/r/toothrot",
        "Discord": "https://discord.gg/DJ8SAr5"
    },
    "paths": {
        "userDir": "docs/user/",
        "devDir": "docs/dev/"
    },
    "code": [
        {
            "extension": "js",
            "language": "javascript",
            "comments": [
                {
                    "type": "single",
                    "start": "^//[ ]{0,1}"
                }
            ]
        },
        {
            "extension": "scss",
            "language": "scss",
            "comments": [
                {
                    "type": "single",
                    "start": "^//[ ]{0,1}"
                }
            ]
        },
        {
            "extension": "php",
            "language": "php",
            "comments": [
                {
                    "type": "single",
                    "start": "^//[ ]{0,1}"
                },
                {
                    "type": "single",
                    "start": "^#[ ]{0,1}"
                }
            ]
        }
    ]
}
